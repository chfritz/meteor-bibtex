	keywords = {adsorption, Apatite, biomaterials, calcium-phosphate nanoparticles, cancer, cell, composite, Drug   delivery, drug-delivery, gel beads, in-vivo, Medical imaging, nanoparticles, Pectin, pectin-coated titanium, receptor, release},
