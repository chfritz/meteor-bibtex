@INPROCEEDINGS{Bichot_etal03,
  author = {Bichot, N. P. and Rossi, A. F. and Desimone, R.},
  title = {Cue, feature and saccade related modulation of neuronal synchronization
	in macaque area {V4} during free-viewing visual search},
  booktitle = {Society for Neuroscience Abstracts},
  year = {2003},
  pages = {551.8},
  abstract = { Although several studies have shown modulation of neural activity
	with attention in area V4 of animals with fixed gaze, the mechanisms
	underlying active exploration of the environment with freely moving
	attention and gaze remain unclear. We investigated the neural mechanisms
	of goal-directed selection in area V4, in macaque monkeys freely
	scanning a visual search array to find a target defined by color,
	shape, or both features (i.e., conjunction). The monkey was shown
	a cue at the start of the trial, which indicated the features of
	the target to be found in the subsequently presented search array.
	After the target was found, the monkey was rewarded for fixating
	it for a prescribed duration. During the cue period, the coherence
	between spikes and the local field potential (LFP) activity was uniformly
	greater for conjunction cues compared to feature cues, and the coherence
	for all cues increased as a given cue was repeated over trials, in
	parallel with superior behavioral performance in both cases. After
	the search array appeared, the spike-field coherence throughout the
	trial was greater at all frequencies when the stimulus inside the
	receptive field matched the features of the searched-for target,
	even when that stimulus was not yet selected for a saccadic eye movement,
	suggesting a bias in favor of neurons representing the target features,
	which persisted until the target was found. Spike-field coherence
	was also further enhanced when the receptive field stimulus was the
	target of the saccade. Thus, the results suggest that the search
	for the target is accomplished by a parallel process, synchronizing
	the activity of neurons representing target features throughout the
	visual field, followed by a serial selection of neurons representing
	candidate targets.}
}

