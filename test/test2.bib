@article{carles_three_2011,
	title = {Three Dimensional Design of Silver Nanoparticle Assemblies Embedded in Dielectrics for Raman Spectroscopy Enhancement and Dark-Field Imaging},
	volume = {5},
	issn = {1936-0851},
	doi = {10.1021/nn202767p},
	abstract = {A strategy to design and fabricate hybrid metallic-dielectric substrates for optical spectroscopy and imaging is proposed. Different architectures consisting of three-dimensional patterns of metallic nanoparticles embedded In dielectric layers are conceived to simultaneously exploit the optical interference phenomenon In stratified media and localized surface plasmon resonances on metal nanoparticles. These structures are based on a simultaneous control of optoelectronic properties at three scales ({3S)} (similar to 2/20/200 nm) and along three directions ({3D).} By ultralow energy ion implantation through a microfabricated stencil we precisely control the size, density, and location of silver nanoparticles embedded in silica/silicon thin films. Elastic (Rayleigh) and inelastic (Raman) scattering imaging assisted by simulations were used to analyze the optical response of these {"3S-3D"} patterned layers. The reflectance contrast is strongly enhanced when resonance conditions between the stationary electromagnetic field in the dielectric matrix and the localized plasmon resonance In the silver nanoparticles are realized. The potential of these {3S-3D} metal-dielectric structures as surface-enhanced Raman scattering substrates Is demonstrated. These novel kinds of plasmonic-photonlc architectures are reproducible and stable; they preserve flat and chemically uniform surfaces, offering opportunities for the development of efficient and reusable substrates for optical spectroscopy and imaging enhancement.},
	language = {English},
	number = {11},
	journal = {Acs Nano},
	author = {Carles, Robert and Farcau, Cosmin and Bonafos, Caroline and Benassayag, Gerard and Bayle, Maxime and Benzo, Patrizio and Groenen, Jesse and Zwick, Antoine},
	month = nov,
	year = {2011},
	keywords = {dark-field imaging, devices, Ion implantation, nanostructures, optical reflectance contrast, optical spectroscopy, plasmon resonance, scattering, sers, silver nanoparticles},
	pages = {8774--8782}
}

@article{catalan_flexoelectric_2011,
	title = {Flexoelectric rotation of polarization in ferroelectric thin films},
	volume = {10},
	issn = {1476-1122},
	doi = {10.1038/NMAT3141},
	abstract = {Strain engineering enables modification of the properties of thin films using the stress from the substrates on which they are grown. Strain may be relaxed, however, and this can also modify the properties thanks to the coupling between strain gradient and polarization known as flexoelectricity. Here we have studied the strain distribution inside epitaxial films of the archetypal ferroelectric {PbTiO3}, where the mismatch with the substrate is relaxed through the formation of domains (twins). Synchrotron X-ray diffraction and high-resolution scanning transmission electron microscopy reveal an intricate strain distribution, with gradients in both the vertical and, unexpectedly, the horizontal direction. These gradients generate a horizontal flexoelectricity that forces the spontaneous polarization to rotate away from the normal. Polar rotations are a characteristic of compositionally engineered morphotropic phase boundary ferroelectrics with high piezoelectricity; flexoelectricity provides an alternative route for generating such rotations in standard ferroelectrics using purely physical means.},
	language = {English},
	number = {12},
	journal = {Nature Materials},
	author = {Catalan, G. and Lubk, A. and Vlooswijk, A. H. G. and Snoeck, E. and Magen, C. and Janssens, A. and Rispens, G. and Rijnders, G. and Blank, D. H. A. and Noheda, B.},
	month = dec,
	year = {2011},
	keywords = {capacitors, dielectric-properties, domain formation, ferroelastic films, morphotropic phase-boundary, origin, perovskite oxides, solid-solutions, strain relaxation, stress},
	pages = {963--967}
}

@inproceedings{cherkashin_reconstruction_2011,
	address = {Stafa-Zurich},
	title = {Reconstruction of a High Angle Tilt (110)/(001) Boundary in Si Using O-lattice Theory},
	volume = {178-179},
	abstract = {High angle close to 90 degrees tilt Si (1 (1) over bar0)//(001) boundary created by direct wafer bonding ({DWB)} using {SmartCut} (R) technology is studied in this work. Experimental identification of defects and morphologies at the interface is realized using conventional transmission electron microscopy ({TEM)} and geometric phase analysis ({GPA)} of high-resolution {TEM} images. Atom reconstruction of the interface along the [1 (1) over bar0]/(001) direction is carried out within the frame of the O-lattice theory. We demonstrate that to preserve covalent bonding across the interface it should consist of \{(1) over bar 11\}//\{(1) over bar 12\} facets intersected by a maximum of six \{1 (1) over bar1\} planes with three 90 degrees Shockley dislocations per facet. For a long enough \{(1) over bar 11\}//\{(1) over bar 12\} interface the formation of Frank dislocations is predicted with a period equal 6 times that of Shockley dislocations. Long range undulations of the interface are shown to be related directly to a deviation from exact 90 degrees tilt of the layer with respect to the substrate.},
