	journal = {Comptes Rendus Physique},
	author = {Hawkes, Peter},
	month = feb,
	year = {2014},
	keywords = {Aberration correctors, Bruit, Correcteurs d'aberrations, Dispositifs à phase, Faisceaux vorticiels, Mirroirs, mirrors, noise, Phase plates, Ptychographie, Ptychography, Vortex beams},
	pages = {110--118}
}

