	number = {1},
	journal = {International Journal of Pharmaceutics},
	author = {Al-Kattan, Ahmed and Girod-Fullana, Sophie and Charvillat, Cedric and Ternet-Fontebasso, Helene and Dufour, Pascal and Dexpert-Ghys, Jeannette and Santran, Veronique and Bordere, Julie and Pipy, Bernard and Bernad, Jose and Drouet, Christophe},
