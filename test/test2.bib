	author = {Matzen, S. and Moussy, J.-B. and Mattana, R. and Bouzehouane, K. and Deranlot, C. and Petroff, F. and Cezar, J. C. and Arrio, M.-A. and Sainctavit, Ph and Gatel, C. and Warot-Fonrose, B. and Zheng, Y.},
