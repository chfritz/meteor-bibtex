	year = {2009},
	keywords = {13th-century, ceramics, copper, glazes, ions, luster pottery, lustre, microstructure, nanoparticle, optical-properties, pottery, shine, silver, tem, Transmission electron microscopy},
	pages = {133--139}
}

@article{serin_tem_2009,
