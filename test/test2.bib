@article{lou_acoustics_2010,
	title = {Acoustics at nanoscale: Raman-Brillouin scattering from thin silicon-on-insulator layers},
	volume = {97},
	issn = {0003-6951},
	shorttitle = {Acoustics at nanoscale},
	doi = {10.1063/1.3499309},
	abstract = {We report on Raman-Brillouin scattering from thin single silicon layers. Starting from a 33 nm silicon-on-insulator structure, a series of layers with progressively decreasing thicknesses was prepared using a chemical treatment consisting of oxide stripping/ formation cycles. In order to determine these thicknesses, experimental Raman-Brillouin spectra are compared to calculations performed in the frame of the photoelastic model. We demonstrate that subnanometer changes in the silicon layer thickness can be derived from a proper analysis of the spectral response. It is shown that a 1 nm thick oxide forms during the chemical treatment. (C) 2010 American Institute of Physics. [doi:10.1063/1.3499309]},
	language = {English},
	number = {14},
	journal = {Applied Physics Letters},
	author = {Lou, N. and Groenen, J. and Benassayag, G. and Zwick, A.},
	month = oct,
	year = {2010},
	pages = {141908}
}

@article{lundin_effect_2010,
