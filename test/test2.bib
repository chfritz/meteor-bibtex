	number = {9},
	journal = {Polymer Journal},
	author = {Silvestre, Clara and Cimmino, Sossio and Pezzuto, Marilena and Marra, Antonella and Ambrogi, Veronica and Dexpert-Ghys, Jeannette and Verelst, Marc and Augier, Sylvain and Romano, Ida and Duraccio, Donatella},
