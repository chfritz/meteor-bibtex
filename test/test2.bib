	keywords = {aglae facility, Antique ceramic, compositional analysis, diffraction, Elemental composition, induced breakdown spectroscopy, ion-beam analysis, libs, pixe, pottery, slips, terra-sigillata, Thin coating},
	pages = {45--52}
