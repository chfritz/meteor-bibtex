	keywords = {compound, crystal, hybrids (cnh2n+1nh3)(2)pbi4, layered-perovskite, material (c6h13nh3)(2)pbi4, optical-properties, quantum wells, semiconductors, spectroscopy, temperature},
	pages = {053521}
