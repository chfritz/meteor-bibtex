	month = dec,
	year = {2009},
	keywords = {electrical-properties, energy ion-implantation, layer, nanostructure, shadow-mask, si, sio2, Stencil mask, Thin film, zno},
	pages = {1044--1047},
	file = {ScienceDirect Full Text PDF:/home/masseboeuf/Taftaf/Biblio et Reviews/Zotero/Dropbox/storage/K9IUU2D9/Barnabé et al. - 2009 - Structured ZnO-based contacts deposited by non-rea.pdf:application/pdf;ScienceDirect Snapshot:/home/masseboeuf/Taftaf/Biblio et Reviews/Zotero/Dropbox/storage/SWT9772G/S0040609009007640.html:text/html}
}

@article{bea_evidence_2009,
	title = {Evidence for Room-Temperature Multiferroicity in a Compound with a Giant Axial Ratio},
	volume = {102},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.102.217603},
	doi = {10.1103/PhysRevLett.102.217603},
	abstract = {In the search for multiferroic materials magnetic compounds with a strongly elongated unit-cell (large axial ratio c/a) have been scrutinized intensely. However, none was hitherto proven to have a switchable polarization, an essential feature of ferroelectrics. Here, we provide evidence for the epitaxial stabilization of a monoclinic phase of {BiFeO3} with a giant axial ratio (c/a=1.23) that is both ferroelectric and magnetic at room temperature. Surprisingly, and in contrast with previous theoretical predictions, the polarization does not increase dramatically with c/a. We discuss our results in terms of the competition between polar and antiferrodistortive instabilities and give perspectives for engineering multiferroic phases.},
	number = {21},
	urldate = {2014-03-04},
	journal = {Physical Review Letters},
	author = {Béa, H. and Dupé, B. and Fusil, S. and Mattana, R. and Jacquet, E. and Warot-Fonrose, B. and Wilhelm, F. and Rogalev, A. and Petit, S. and Cros, V. and Anane, A. and Petroff, F. and Bouzehouane, K. and Geneste, G. and Dkhil, B. and Lisenkov, S. and Ponomareva, I. and Bellaiche, L. and Bibes, M. and Barthélémy, A.},
	month = may,
	year = {2009},
	pages = {217603}
}

@article{ben_ahmed_crystal_2009,
	title = {Crystal structure, vibrational spectra and theoretical studies of L-histidinium dihydrogen phosphate-phosphoric acid},
	volume = {920},
	issn = {0022-2860},
	doi = {10.1016/j.molstruc.2008.09.029},
	abstract = {In this work. we report a combined experimental and theoretical study on molecular structure and vibrational spectra of L-histidinium dihydrogen phosphate-phosphoric acid, with particular emphasize on the correlation between the intermolecular hydrogen bonds and the hyperpolarizability. Single crystal of L-histidinium dihydrogen phosphate-phosphoric acid has been subjected to X-ray diffraction and Raman spectroscopy. The title compound crystallises in the non-centrosymmetric space group P2(1). Raman spectra have been recorded in the frequency range 1150-3350 cm(-1)]. To obtain a more reliable assignment of the Raman and {IR} spectra. we have calculated the geometry and the frequencies using {HF} and {DFT} methods. All the experimental vibrational bands have been discussed and assigned to normal mode or to combinations and overtones on the basis of our calculations. The optimized geometric bond lengths and bond angles obtained by using {HF} and {DFT} ({B3LYP} and {BLYP)} are in well agreement with the experimental data. The results of {DFT-B3LYP} method have shown better fit to experimental ones than {HF} in evaluating vibrational frequencies. To investigate microscopic second order non-linear optical behaviour of the examined complex, the electric dipole mu, the polarizability alpha and the hyperpolarizability beta were computed using {DFr//B3LYP/6-31} G(d) method. According to our calculation, the title compound exhibits non-zero beta value revealing microscopic second order {NLO} behaviour. (C) 2008 Elsevier {B.V.} All rights reserved.},
	language = {English},
	number = {1-3},
	journal = {Journal of Molecular Structure},
	author = {Ben Ahmed, A. and Feki, H. and Abid, Y. and Boughzala, H. and Minot, C. and Mlayah, A.},
	month = feb,
	year = {2009},
	keywords = {crystal structure, dft, growth, Hyperpolarizability, nlo, nonlinear-optical properties, Vibrational assignment},
	pages = {1--7}
}

@article{bonell_mgo-based_2009,
	title = {{MgO-Based} Epitaxial Magnetic Tunnel Junctions Using Fe-V Electrodes},
	volume = {45},
	issn = {0018-9464},
	doi = {10.1109/TMAG.2009.2022644},
	abstract = {To examine the influence of the barrier quality in fully epitaxial {Fe/MgO/Fe(001)} magnetic tunnel junctions ({MTJs)}, we propose to use Fe-V alloys as magnetic electrodes. This leads to a reduced misfit with {MgO.} We actually observe, by high-resolution electron microscopy ({HREM)} and local strain measurements, that the misfit dislocations density in the {MgO} barrier is lower when it is grown on Fe-V(001). This improvement of the crystalline quality of the {MgO} barrier actually leads to a significant increase of the tunnel magneto-resistance ({TMR)}, despite the loss of spin polarization ({SP)} in these alloys, which was measured by spin-polarized X-ray photoelectron spectroscopy ({SR-XPS).}},
	language = {English},
	number = {10},
	journal = {Ieee Transactions on Magnetics},
	author = {Bonell, Frederic and Andrieu, Stephane and Bertran, Francois and Lefevre, Patrick and Ibrahimi, Amina Taleb and Snoeck, Etienne and Tiusan, Coriolan-Viorel and Montaigne, Francois},
	month = oct,
	year = {2009},
	keywords = {dislocation, epitaxial magnetic tunnel junction, Fe-V alloy, magnetoresistance, mgo, room-temperature, spin   polarization, strain},
	pages = {3467--3471}
}

