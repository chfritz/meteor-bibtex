@article{ibrahim_carbon_2013,
	title = {Carbon Coating, Carburization, and High-Temperature Stability Improvement of Cobalt Nanorods},
	volume = {117},
	issn = {1932-7447},
	doi = {10.1021/jp3125457},
	abstract = {The reactivity of highly crystalline hcp cobalt nanorods ({NRs)} with organic solvents at high temperature was studied. Cobalt {NRs} with a mean diameter of 15 nm were first synthesized by the polyol process and then heated to 300 degrees C in octadecene ({ODE)}, oleylamine ({OA)}, or mixtures of these two solvents. The surface and structural modifications of the Co {NRs} were characterized by Raman spectroscopy, X-ray photoelectron spectroscopy ({XPS)}, X-ray diffraction ({XRD)}, and scanning and transmission electron microscopy ({SEM} and {TEM).} A disordered carbon shell was formed at the cobalt rod surface, the thickness of which can be tuned from 5 to 25 nm by increasing the amount of oleylamine in the solvent mixture. This carbon shell partially reduced the native cobalt oxide observed at the surface of the {NRs} and drastically improved their temperature stability as inferred from in situ {XRD} study and {TEM.} The shape anisotropy and the crystallite anisotropy of the hcp phase are both preserved up to 400 degrees C for the carbon-coated cobalt rods, whereas the uncoated {NRs} lose their anisotropy at 225 degrees C. Treatments at 300 C in {ODE/OA} mixtures for different durations allowed the progressive carburization of Co to {Co2C.} The crystallographic orientation of the {Co2C} grains within the cobalt {NRs} combined with the different carbon-shell thickness on the \{10 (1) over bar0\} and (0001) facets of the rods suggested a preferential carburization from the lateral facets of the hcp cobalt rods.},
	language = {English},
	number = {30},
	journal = {Journal of Physical Chemistry C},
	author = {Ibrahim, Mona and Marcelot-Garcia, Cecile and Atmane, Kahina Ait and Berrichi, Elcrame and Lacroix, Lise-Marie and Zwick, Antoine and Warot-Fonrose, Benedicte and Lachaize, Sebastien and Decorse, Philippe and Piquemal, Jean-Yves and Viau, Guillaume},
	month = aug,
	year = {2013},
	keywords = {catalytic method, co, diffraction, graphite shells, iron, magnetic-properties, nanocrystals, nanoparticles, particles, size},
	pages = {15808--15816}
}

@article{jaegeler-hoheisel_plasmonic_2013,
	title = {Plasmonic Shaping in Gold Nanoparticle Three-Dimensional Assemblies},
	volume = {117},
	issn = {1932-7447},
	doi = {10.1021/jp406410k},
	abstract = {When a large number of similar gold particles are organized into complex architectures, the dipolar plasmon spectrum of the individual plasmonic entities gives rise to a broader, red-shifted feature centered around 750 nm. In this work, we show that superstructures fabricated using the convective assisted capillary force assembly method ({CA-CFA)} and excited at that wavelength display a subwavelength patterning of their optical field intensity that results from the self-consistent coupling between the colloidal nanoparticles. First, we demonstrate the fabrication of shape-controlled three-dimensional assemblies of metallic nanocrystals using the {CA-CFA} method. In a second step, the absorption band resulting from the mutual coupling between the metallic building blocks is exploited to excite a coupled plasmon mode and map the twopphoton luminescence ({TPL)} by scanning a tightly focused light beam. Highly resolved {TPL} images show that the morphology of the plasmonic particle assemblies has a strong impact on their optical response. A model based on a rigorous optical Gaussian beam implementation inside a generalized propagator derived from a three-dimensional Green dyadic function accurately reproduces the {TPL} maps revealing the influence of interparticle separation and thus coupling between the individual particles. Finally, we show that the spatial distribution of the electric field intensity can be controlled by tuning the linear polarization of the optical excitation.},
	language = {English},
	number = {44},
	journal = {Journal of Physical Chemistry C},
	author = {Jaegeler-Hoheisel, Till and Cordeiro, Julien and Lecarme, Olivier and Cuche, Aurelien and Girard, Christian and Dujardin, Erik and Peyrade, David and Arbouet, Arnaud},
	month = nov,
	year = {2013},
	keywords = {excitation, fano resonances, light, metamaterials, microscopy, modes, nanoprisms, nanostructures, networks, surface-plasmon},
	pages = {23126--23132}
}

@article{kallel_photoluminescence_2013,
	title = {Photoluminescence enhancement of silicon nanocrystals placed in the near field of a silicon nanowire},
	volume = {88},
	issn = {1098-0121},
	doi = {10.1103/PhysRevB.88.081302},
	abstract = {Semiconductor nanowires have an excellent ability to trap, guide, scatter, or absorb light for specific morphology-dependent resonant optical modes. The electromagnetic field enhancement associated with these modes could be used to modify the luminescence of emitters positioned in the vicinity of the nanowire, in a way similar to plasmonic nanostructures. We show that the photoluminescence of a single plane of silicon nanocrystals in silica, positioned at about 3 nm below the surface, can be enhanced by a factor of 2 to 3 in the presence of a silicon nanowire antenna on the silica surface. This could be the basis of a promising fully complementary metal oxide semiconductor compatible process to improve silicon-based light emitting devices, despite a lower enhancement compared to plasmonic nanostructures. Two-dimensional photoluminescence maps recorded for different polarization configurations (incident electric field parallel or perpendicular to the nanowire axis) exhibit different behaviors and can be related to the electric field intensity distribution in the near-field region of the nanowire, where the active silicon nanocrystal layer is located.},
	language = {English},
	number = {8},
	journal = {Physical Review B},
	author = {Kallel, Houssem and Arbouet, Arnaud and Carrada, Marzia and Ben Assayag, Gerard and Chehaidar, Abdallah and Periwal, Priyanka and Baron, Thierry and Normand, Pascal and Paillard, Vincent},
	month = aug,
	year = {2013},
	keywords = {cells, devices, emission, ion-beam-synthesis, light confinement, scattering, si nanocrystals},
	pages = {081302}
}

@article{kallel_enhanced_2013,
	title = {Enhanced absorption of solar light in {Ge/Si} core-sheath nanowires compared to {Si/Ge} core-sheath and Si1-{xGex} nanowires: A theoretical study},
	volume = {114},
	issn = {0021-8979},
	shorttitle = {Enhanced absorption of solar light in {Ge/Si} core-sheath nanowires compared to {Si/Ge} core-sheath and Si1-{xGex} nanowires},
	doi = {10.1063/1.4846755},
	abstract = {Coaxial core-sheath semiconductor nanowires exhibit an improved light absorption efficiency, rendering them promising building blocks in photovoltaic devices. In the present theoretical work, we focus on the optical properties of {Si/Ge} and {Ge/Si} core-sheath nanowires, compared to Si1-{xGex} alloy variants (without coated shell) of the same composition in Si and Ge. Using the analytical Lorenz-Mie theory, we compute both the absorption and scattering efficiencies, with a particular attention devoted to the shell thickness influence. The leaky modes resonances, associated to the optical response enhancement, are investigated and compared for all core/sheath and alloy variants. The optical resonances are found to keep almost the same spectral positions regardless the nanowire structure. More interestingly, core-sheath nanowires absorb more efficiently visible to near-infrared light than Si1-{xGex} alloys. Finally, the integrated solar absorption J(solar), which is a figure of merit proportional to the photocurrent generated in a nanowire, is more efficient in the case of {Ge/Si} core-sheath nanowires than in the two other structures. (C) 2013 {AIP} Publishing {LLC.}},
	language = {English},
	number = {22},
	journal = {Journal of Applied Physics},
	author = {Kallel, Houssem and Chehaidar, Abdallah and Arbouet, Arnaud and Paillard, Vincent},
	month = dec,
	year = {2013},
	keywords = {cells, design principles, devices, heterostructures, p-n-junctions, shell, silicon nanowires},
	pages = {224312}
}

@article{khomenkova_undoped_2013,
	title = {Undoped and Nd3+ doped Si-based single layers and superlattices for photonic applications},
	volume = {210},
	issn = {1862-6300},
	doi = {10.1002/pssa.201200942},
	abstract = {This work presents the benefits of the superlattice approach to control light emission properties of materials with Si nanoclusters and rare-earth ions. The undoped and Nd3+-doped both Si-rich-{SiO2} single layers and Si-rich-{SiO2/SiO2} superlattices were grown by radio frequency magnetron sputtering. Their properties were investigated by means of spectroscopic ellipsometry, Fourier infrared transmission spectroscopy, transmission electron microscopy, and photoluminescence ({PL)} methods versus deposition conditions, annealing treatment, and superlattice design (doping and thickness of alternated sublayers). An intense Nd3+ emission from as-deposited single layers and superlattices was observed. The lower annealing temperature (below 900 degrees C) of the single layers and superlattices favors the formation of amorphous Si clusters that act as effective sensitizers of rare-earth ions. The highest Nd3+ {PL} intensity was achieved after a conventional annealing at about 600-800 degrees C in nitrogen flow for all samples. Crystallized Si-nanoclusters were formed in Si-rich-{SiO2} single layers upon annealing at 1000-1100 degrees C, whereas their formation in the superlattices occurred at higher temperatures (1100-1150 degrees C). The mechanism of Nd ions' excitation via energy transfer from Si-nanoclusters and/or matrix defects, if any, is discussed.},
	language = {English},
	number = {8},
	journal = {Physica Status Solidi a-Applications and Materials Science},
	author = {Khomenkova, Larysa and Labbe, Christophe and Portier, Xavier and Carrada, Marzia and Gourbilleau, Fabrice},
	month = aug,
