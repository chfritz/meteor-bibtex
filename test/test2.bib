	booktitle = {Transmission Electron Microscopy in Micro-Nanoelectronics},
	publisher = {John Wiley \& Sons, Inc.},
	author = {Hÿtch, Martin and Houdellier, Florent and Cherkashin, Nikolay and {ReboH}, Shay and Javon, Elsa and Benzo, Patrick and Gatel, Christophe and Snoeck, Etienne and Claverie, Alain},
