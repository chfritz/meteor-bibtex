	month = mar,
	year = {2013},
	keywords = {bottom electrode, ferroelectric properties, growth, lanio3 thin-films, memory applications, microstructure, phase-transition, pulsed-laser deposition, si, ysz films},
	pages = {112905}
}

@article{seguini_scaling_2013,
	title = {Scaling size of the interplay between quantum confinement and surface related effects in nanostructured silicon},
	volume = {103},
	issn = {0003-6951},
	doi = {10.1063/1.4813743},
	abstract = {Si nanocrystals ({NCs)} embedded in a {SiO2} matrix provide an exemplar curved nanostructured interface to evidence the competition between surface states and quantum confinement ({QC)} effects. The study of the energy band alignment as a function of {NCs} size ({\textless}5 nm) clarifies their interplay and identifies, with subnanometric resolution, three different regimes. Primarily {QC} affects the conduction band, then surface effects pin the conduction states, and finally {QC} starts to modify the valence band. A way to study how different nanoscale configurations compete with pure quantum properties is established. (C) 2013 {AIP} Publishing {LLC.}},
	language = {English},
