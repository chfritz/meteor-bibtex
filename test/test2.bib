	keywords = {electroluminescence, multilayers, nanoclusters, nanocrystals, optical-properties, optoelectronic applications, photoluminescence, rare-earth ions, {RF} magnetron sputtering, rich silicon-oxide, silicon   nanostructures, si/sio2 superlattices, thin-films, visible-light emission},
	pages = {1532--1543}
}

